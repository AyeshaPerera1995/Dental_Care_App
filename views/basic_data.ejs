    <div class="card-body pt-2">


<!-- ********************************************************************************************* -->
<% for(var i = 0 ; i < brandlist.length ; i++){ %>
        <hr>	
        <div class="d-flex flex-wrap align-items-center">
            <div class="d-flex flex-column flex-grow-1 pr-3">
                <a class="text-dark-75 text-hover-primary font-size-lg"><%=brandlist[i].brand_name%></a>
            </div>
            <div class="d-flex align-items-center py-lg-0 py-2">
                <div class="d-flex flex-column">
                    <% if(brandlist[i].Status == 'Active'){ %>
                        <a href="/basic_data/set_brand_status/<%=brandlist[i].id%>&<%='In-Active'%>" class="label label-lg label-light label-inline font-weight-bold py-4" style="background: green; color: whitesmoke;">Active</a>
