                        <a href="/basic_data/set_brand_status/<%=brandlist[i].id%>&<%='Active'%>" class="label label-lg label-light label-inline font-weight-bold py-4" style="background: rgb(233, 79, 79); color: whitesmoke;">Deactive</a>
                    <% } %>
                </div>
            </div>
        </div>
<% } %>
<!-- ******************************************************************************************************* -->

