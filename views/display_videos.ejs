	    <script>
			var voiceList = document.querySelector('#voiceList');
			var btnSpeak = document.querySelector('#Numberz');
			var synth = window.speechSynthesis;
			var synth1 = window.speechSynthesis;
			var voices = [];
	
			PopulateVoices();
			if(speechSynthesis !== undefined){
				speechSynthesis.onvoiceschanged = PopulateVoices;
			}
	
			
				
			
	
			function PopulateVoices(){
				voices = synth.getVoices();
			}

		  	const socket = io('http://137.116.141.10:5455/');
		  	socket.on("connection", (socket) => {
  				console.log(socket.id);
			});

			socket.on('NumberListner', function(data) {
				document.getElementById("Numberz").innerHTML = data.appointment_number;				
				var toSpeak = new SpeechSynthesisUtterance('Token Number '+data.appointment_number+' please proceed to room number 1');
				var selectedVoiceName = 'Google UK English Female';
				voices.forEach((voice)=>{
					if(voice.name === selectedVoiceName){
						toSpeak.voice = voice;
					}
				});
				synth.speak(toSpeak);
			});


			socket.on('NumberRedListner', function(data) {
				console.log(data.No.RedNumbers);
				 document.getElementById("RedNumberz").innerHTML = data.No.RedNumbers;				
				var toSpeak1 = new SpeechSynthesisUtterance('Red Number '+data.No.RedNumbers+' please proceed to room number 1');
				var selectedVoiceName1 = 'Google UK English Female';
				voices.forEach((voice)=>{
					if(voice.name === selectedVoiceName1){
						toSpeak1.voice = voice;
					}
				});
				synth1.speak(toSpeak1);
			});

		</script>

